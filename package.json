{
  "name": "musedb-web",
  "version": "1.0.0",
  "description": "Web app to view and interact with MuseDB database",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:python": "concurrently \"npm run server:python\" \"npm run client\"",
    "server": "node server/index.js",
    "server:python": "cd server && python app.py",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

